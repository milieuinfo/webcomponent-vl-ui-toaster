{
  "name": "vl-ui-toaster",
  "version": "0.0.0",
  "description": "vl-ui-toaster",
  "main": "vl-toaster",
  "scripts": {
    "prepare": "npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-toaster.js -- *.src.js",
    "release:prepare:imports": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-toaster.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-toaster/style.css' vl-toaster.js",
    "release:prepare:commit": "git add -f vl-toaster.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "vl-ui-core": "1.4.4"
  },
  "peerDependencies": {
    "vl-ui-core": "1.4.4"
  },
  "devDependencies": {
    "@govflanders/vl-ui-alert": "3.7.2",
    "@govflanders/vl-ui-button": "3.7.2",
    "@govflanders/vl-ui-core": "3.7.2",
    "@govflanders/vl-ui-icon": "3.7.2",
    "@govflanders/vl-ui-toaster": "3.7.2",
    "@govflanders/vl-ui-util": "3.7.2",
    "np": "3.0.4",
    "replace": "1.0.0",
    "uglify-es": "github:mishoo/UglifyJS2#harmony",
    "vl-ui-alert": "^1.3.3",
    "vl-ui-button": "1.1.3",
    "vl-ui-util": "^1.2.15",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-toaster.git"
  },
  "author": "DIDM",
  "license": "MIT",
